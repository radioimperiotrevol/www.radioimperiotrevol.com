	<script type="text/javascript">
	$(document).ready(function(){
		//Add New
		$(document).on('click', '#addnew', function(){
			$cancion=$('#cancion').val();
			$habbo=$('#habbo').val();				
			if ($('#cancion').val()=="" || $('#habbo').val()==""){
				Swal.fire({
						  position: 'center',
						  icon: 'error',
						  title: 'Por favor, completa ambos campos',
						  showConfirmButton: false,
						  timer: 1500
						});
			}
			else{
						
				$.ajax({
					type: "POST",
					url: "addnew.php",
					data: {
						cancion: $cancion,
						habbo: $habbo,
						add: 1,
					},
					success: function(){
						  Swal.fire({
						  position: 'center',
						  icon: 'success',
						  title: 'En pocos minutos el DJ va a poner esa mondá',
						  showConfirmButton: false,
						  timer: 1500
						});
						$('#cancion').val("");
						$('#habbo').val("");
						
					}
				});
			}
		});
		});
</script>


